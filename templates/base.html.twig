<!doctype html>
<html lang="en">
	<head>
		<meta charset="utf-8">
		<meta http-equiv="X-UA-Compatible" content="IE=chrome">
		<meta
		name="viewport" content="width=device-width, initial-scale=1">
		<!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
		<meta name="description" content="">
		<meta name="author" content="">
		<link rel="icon" href="../../favicon.ico">
		<link rel="canonical" href="https://getbootstrap.com/docs/3.4/examples/starter-template/">
		<title>
			{% block title %}Clowns Store
			{% endblock %}
		</title>
		{% block stylesheets %}
			{# <link rel="stylesheet" href="{{asset('build/app.css')}}"> #}
		{% endblock %}
		<link href="https://fonts.googleapis.com/css2?family=Bubblegum+Sans&family=Nanum+Pen+Script&display=swap" rel="stylesheet">
		<link
		rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css"/>
		<!-- CSS only -->
		<link
		href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0-beta1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-0evHe/X+R7YkIZDRvuzKMRqM+OrBnVFBL6DOitfPri4tjfHxaWutUpFmBp4vmVor" crossorigin="anonymous">
		<!-- JavaScript Bundle with Popper -->
		<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0-beta1/dist/js/bootstrap.bundle.min.js" integrity="sha384-pprn3073KE6tl6bjs2QrFaJGz5/SUsLqktiwsUTF55Jfv3qYSDhgCecCxMW52nD2" crossorigin="anonymous"></script>
		<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.8.3/font/bootstrap-icons.css">

	</head>

	<!-- Body -->

	<body>

		<!-- navbar -->
		<nav class="navbar navbar-expand-md navbar-white bg-white container ">
			<div class="container-fluid ">
				<a class="navbar-brand fs-2 " style="font-weight:bolder; font-family: 'Bubblegum Sans', cursive; " href="{{ path('app_home') }}">Clowns Store</a>
				<button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#mynavbar">
					<span class="navbar-toggler-icon"></span>
				</button>
				<div
					class=" navbar" id="mynavbar">
					<!-- End Home Navbar -->
					{% if app.user %}
						<!-- Product Navbar -->
						<div class="collapse navbar-collapse d-flex justify-content-around " id="navbarNavDarkDropdown">
							<ul
								class="navbar-nav">
								<!-- Home Navbar -->
								<li>
									<a class="nav-link fs-4" onmouseover="navItemOver(this)" onmouseout="navItemOut(this)" style="font-family: 'Bubblegum Sans', cursive; color:#000;" id="navbarElement" href="{{ path('app_home') }}">Home</a>
								</li>
							</ul>
							<ul class="navbar-nav">
								<li class="nav-item dropdown ">
									<a class="nav-link dropdown-toggle fs-4" onmouseover="navItemOver(this)" onmouseout="navItemOut(this)" style="font-family: 'Bubblegum Sans', cursive; color:#000;" href="#" id="navbarDarkDropdownMenuLink" role="button" data-bs-toggle="dropdown" aria-expanded="false">
										Products
									</a>
									<ul class="dropdown-menu dropdown-menu-white text-center" aria-labelledby="navbarDarkDropdownMenuLink">
										{% if is_granted('ROLE_ADMIN') %}
											<li>
												<a class="dropdown-item fs-5" onmouseover="navItemOver(this)" onmouseout="navItemOut(this)" style="font-family: 'Bubblegum Sans', cursive; color:#000;" href="{{ path('product_create') }}">Add Product</a>
											</li>
										{% endif %}
										<li>
											<a class="dropdown-item fs-5" onmouseover="navItemOver(this)" onmouseout="navItemOut(this)" style="font-family: 'Bubblegum Sans', cursive; " href="{{ path('product_list') }}">View Product</a>
										</li>
									</ul>
								</li>
							</ul>
							<!-- End Product Navbar -->

							<!-- Category Navbar -->
							<ul class="navbar-nav">
								<li class="nav-item dropdown">
									<a class="nav-link dropdown-toggle fs-4 " onmouseover="navItemOver(this)" onmouseout="navItemOut(this)" style="font-family: 'Bubblegum Sans', cursive; color:#000;" href="#" id="navbarDarkDropdownMenuLink" role="button" data-bs-toggle="dropdown" aria-expanded="false">
										Categories
									</a>
									<ul class="dropdown-menu dropdown-menu-white" aria-labelledby="navbarDarkDropdownMenuLink">
										{% if is_granted('ROLE_ADMIN') %}
											<li>
												<a class=" text-center dropdown-item fs-5" onmouseover="navItemOver(this)" onmouseout="navItemOut(this)" style="font-family: 'Bubblegum Sans', cursive; color:#000;" href="{{ path('category_create') }}">Add Category</a>
											</li>
										{% endif %}
										<li>
											<a class=" text-center dropdown-item fs-5" onmouseover="navItemOver(this)" onmouseout="navItemOut(this)" style="font-family: 'Bubblegum Sans', cursive; color:#000;" href="{{ path('category_list') }}">View Category</a>
										</li>
									</ul>
								</li>
							</ul>
							<!-- End Category Navbar -->
							<!-- Manufacturer Navbar -->
							<ul class="navbar-nav">
								<li class="nav-item dropdown">
									<a class="nav-link dropdown-toggle fs-4" onmouseover="navItemOver(this)" onmouseout="navItemOut(this)" style="font-family: 'Bubblegum Sans', cursive; color:#000;" href="#" id="navbarDarkDropdownMenuLink" role="button" data-bs-toggle="dropdown" aria-expanded="false">
										Manufacturer
									</a>
									<ul class="dropdown-menu dropdown-menu-white" aria-labelledby="navbarDarkDropdownMenuLink">
										{% if is_granted('ROLE_ADMIN') %}
											<li>
												<a class="dropdown-item fs-5" onmouseover="navItemOver(this)" onmouseout="navItemOut(this)" style="font-family: 'Bubblegum Sans', cursive; color:#000;" href="{{ path('manufacturer_create') }}">Add Manufacturer</a>
											</li>
										{% endif %}
										<li>
											<a class="dropdown-item fs-5 " onmouseover="navItemOver(this)" onmouseout="navItemOut(this)" style="font-family: 'Bubblegum Sans', cursive; color:#000;" href="{{ path('manufacturer_list') }}">View Manufacturer</a>
										</li>
									</ul>
								</li>
							</ul>
							<!-- End Manufacturer Navbar -->
							<ul class="navbar-nav">
								<a class="nav-link  fs-4 " onmouseover="navItemOver(this)" onmouseout="navItemOut(this)" style="font-family: 'Bubblegum Sans', cursive; color:#000;" href="{{ path('app_logout') }}">Log Out</a>
							</ul>

						</ul>
					{% else %}
						<ul class="navbar-nav">
							<a class="nav-link  fs-4 " onmouseover="navItemOver(this)" onmouseout="navItemOut(this)" style="font-family: 'Bubblegum Sans', cursive; color:#000;" href="{{ path('app_login') }}">Log In</a>
						</ul>
						<ul class="navbar-nav">
							<a class="nav-link  fs-4 " onmouseover="navItemOver(this)" onmouseout="navItemOut(this)" style="font-family: 'Bubblegum Sans', cursive; color:#000;" href="{{ path('app_register') }}">Register</a>
						</ul>
					{% endif %}
				</div>
			</div>
			<form class="d-flex">
				<input class="form-control me-2" type="text" style="font-family: 'Bubblegum Sans', cursive;" placeholder="Search">
				<button class="btn btn-outline-dark btn-sm " style="font-family: 'Bubblegum Sans', cursive;" type="button">Search</button>
			</form>
		</nav>
		{% if app.user %}
			<div class="text-dark mb-3">
				You are logged in as
				{{ app.user.userIdentifier }},
				<a href="{{ path('app_logout') }}">Logout</a>
			</div>
		{% endif %}
		<hr style=" width:50%; position:relative; left:25%; border-top: 2px solid; color:#000000 ">
		<!-- navbar -->
		<!-- Carousel -->
			<div class="" style=""> <div class="carousel" style="padding-top: 1.5%; padding-bottom: 3%; ">
				<div id="carouselExampleControls" class="carousel slide h-50" data-bs-ride="carousel">
					<div class="carousel-inner">
						<div class="carousel-item active h-50"><img src="https://tinyurl.com/3xmd7wnp" class="d-inline-flex w-100 " style="height:700px; object-fit: cover;" alt="..."></div>
						<div class="carousel-item"><img src="https://tinyurl.com/mptvr4c9" class="d-inline-flex w-100 " style="height: 700px; object-fit: cover;" alt="..."></div>
						<div class="carousel-item"><img src="https://tinyurl.com/4brww2bv" class="d-inline-flex w-100" style="height: 700px; object-fit: cover;" alt="..."></div>
					</div>
					<button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleControls" data-bs-slide="prev">
						<span class="carousel-control-prev-icon" aria-hidden="true"></span>
						<span class="visually-hidden">Previous</span>
					</button>
					<button class="carousel-control-next" type="button" data-bs-target="#carouselExampleControls" data-bs-slide="next">
						<span class="carousel-control-next-icon" aria-hidden="true"></span>
						<span class="visually-hidden">Next</span>
					</button>
				</div>
			</div>
			<div>
				<h1 style="text-align:center; padding: 2%;">Our Development Team Member</h1>
			</div><!-- team-member --><div class="container team-member">
				<div class="row">
					<div class="card-group ">
						<div class="card col-sm" style="margin: 1.5% 1.5%; border:none;">
							<img src="https://tinyurl.com/2kdcc3j2" class="card-img-top" style="border-radius:50%;" alt="..."><div class="card-body">
								<h3 class="card-title">Front-end Coppier</h3>
								<h5 class="card-title">Doan Tran Vinh Kha</h5>
								<p class="card-text">I'm the front-end developer who is very talented in using StackOverFlow.</p>
								<p class="card-text">
									<small class="text-muted">Last updated 3 mins ago</small>
								</p>
							</div>
						</div>
						<div class="card col-sm" style="margin: 1.5% 1.5%; border:none;"><img src="https://tinyurl.com/3duwdr87" class="card-img-top" style="border-radius:50%;" alt="..."><div class="card-body">
								<h3 class="card-title">Back-end Duplicater</h3>
								<h5 class="card-title">Dinh Gia Bao</h5>
								<p class="card-text">I do not angry when people copy my source code because it is actually not my source code.</p>
								<p class="card-text">
									<small class="text-muted">Last updated 3 mins ago</small>
								</p>
							</div>
						</div>
						<div class="card col-sm" style="margin: 1.5% 1.5%;border:none;"><img src="https://tinyurl.com/5n6u8twx" class="card-img-top" style="border-radius:50%;" alt="..."><div class="card-body">
								<h3 class="card-title">Full Stack Imitater</h3>
								<h5 class="card-title">Nguyen Thi Khanh Nam
								</h5>
								<p class="card-text">Look at my picture, this is how my code look like - Hillarious</p>
								<p class="card-text">
									<small class="text-muted">Last updated 3 mins ago</small>
								</p>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
		<div class="container">
			<div class="row">
				<div class="col-md-12">
					{% for flash_message in app.session.flashBag.get('notice') %}
						<div class="alert alert-success">{{ flash_message }}</div>
					{% endfor %}
					{% for flash_message in app.session.flashBag.get('error') %}
						<div class="alert alert-danger">{{ flash_message }}</div>
					{% endfor %}
					{% block body %}{% endblock %}
				</div>
			</div>
		</div>
	</body><!--container -->{% block javascripts %}{% endblock %}
	<hr style=" width:70%; position:relative; left:15%; border-top: 2px solid; color:#000000 ">
	<footer class="bg-white text-center text-dark">
		<div class="containerp-4"><!-- Section: Social media --><sectionclass="mb-4">
				<!-- Facebook -->
				<a class="btn btn-outline-dark m-1" href="#!" role="button" style="border-radius: 20%; border: 2px solid white;">
					<i class="fab fa-facebook-f"></i>
				</a><!-- Twitter --><a class="btn btn-outline-dark  m-1" href="#!" role="button" style="border-radius:20%; border: 2px solid white;">
					<i class="fab fa-twitter"></i>
				</a><!-- Google --><a class="btn btn-outline-dark  m-1" href="#!" role="button" style="border-radius: 20%; border: 2px solid white;">
					<i class="fab fa-google"></i>
				</a><!-- Instagram --><a class="btn btn-outline-dark  m-1" href="#!" role="button" style="border-radius: 20%; border: 2px solid white;">
					<i class="fab fa-instagram"></i>
				</a><!-- Linkedin --><a class="btn btn-outline-dark  m-1" href="#!" role="button" style="border-radius: 20%; border: 2px solid white;">
					<i class="fab fa-linkedin-in"></i>
				</a><!-- Github --><a class="btn btn-outline-dark m-1" href="#!" role="button" style="border-radius: 20%; border: 2px solid white;">
					<i class="fab fa-github"></i>
				</a>
			</section><!-- Section: Social media --><!-- Section: Text --><section class="mb-4">
				<p class="fw-bold">This project is our team efforts, we have tried our best and hope to bring you the best experience!</p>
			</section>
		</div>
		<!-- Copyright -->
		<div class="text-center p-3 text-white" style="background-color:#000000;">Â© 2022 Copyright:
			<a class="text-white" href="{{ path('app_home') }}">
				Triple Clowns</a>
		</div>
		<!-- Copyright -->
	</footer><!-- Footer --></html>
<script>
	function navItemOver(x) {
x.style.backgroundColor = '#000';
x.style.color = "#ffffff";
x.style.borderRadius = "16px";
}

function navItemOut(x) {
x.style.backgroundColor = '#ffffff';
x.style.color = '#000';
}
</script>
